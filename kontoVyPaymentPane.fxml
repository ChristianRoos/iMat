<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane fx:id="accountPayment" prefHeight="230.0" prefWidth="480.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="accountPaymentLabel" layoutX="101.0" text="Betalningsuppgifter" underline="true">
         <font>
            <Font size="32.0" />
         </font>
      </Label>
      <Button fx:id="accountPaymentButtonDone" layoutX="158.0" layoutY="115.0" mnemonicParsing="false" style="-fx-background-color: d31146;" text="Lägg till betalkort" textFill="WHITE">
         <font>
            <Font size="18.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Label layoutX="57.0" layoutY="68.0" text="Du har för tillfället inte lagt till något betalkort">
         <font>
            <Font name="System Italic" size="18.0" />
         </font>
      </Label>
   </children>
</Pane>
